port 1194
proto udp
dev tun
ca ca.crt
cert vpn01.crt
key vpn01.pem  # This file should be kept secret
extra-certs subca.crt
dh dh2048.pem
server 192.168.10.0 255.255.255.0
route 172.23.128.0 255.255.224.0
#push "redirect-gateway def1 bypass-dhcp"
push "dhcp-option DNS 10.29.112.38" # DNS Server
push "dhcp-option NTP 10.29.112.38" # NTP Server
push "route 10.29.111.0 255.255.255.224" # Mycity Production
push "route 10.29.109.160 255.255.255.224" # Prod Proxy Subnet
push "route 172.23.160.0 255.255.224.0" # vpn02 net range                                                                            
push "route 172.23.192.0 255.255.224.0" # vpn03 net range 
push "route 172.23.224.0 255.255.224.0" # vpn entry range
client-config-dir ccd
ccd-exclusive
client-to-client
keepalive 10 300
user openvpn
group nogroup
persist-key
persist-tun
status /var/log/openvpn/openvpn-status.log
log-append  /var/log/openvpn/openvpn.log
verb 6
management localhost 7777
remote-cert-tls client
#ns-cert-type client
explicit-exit-notify 1
tls-version-min 1.2
