port 1194
proto udp
dev tun
ca ca.crt
cert vpnentry.crt
key vpnentry.pem  # This file should be kept secret
extra-certs subca.crt
remote-cert-tls client
dh dh2048.pem
topology subnet
server 172.23.224.0 255.255.224.0
ifconfig-pool-persist /var/log/openvpn/ipp.txt
push "route 172.23.128.0 255.255.224.0" # vpn01 net range
push "route 172.23.160.0 255.255.224.0" # vpn02 net range
push "route 172.23.192.0 255.255.224.0" # vpn03 net range
push "dhcp-option DNS 10.29.112.38"
keepalive 10 120
comp-lzo
user  openvpn
group openvpn
persist-key
persist-tun
status /var/log/openvpn/openvpn-status.log
log-append  /var/log/openvpn/openvpn.log
verb 6
explicit-exit-notify 1
management localhost 7777
script-security 2
learn-address /etc/openvpn/setrules.sh
