dev tun
port 1194
proto udp
server 172.23.32.0 255.255.224.0
ca ca.crt
cert vpn02-test.crt
key vpn02-test.pem  
extra-certs subca.crt
dh dh2048.pem
#topology net30
client-config-dir ccd
ccd-exclusive
#push "redirect-gateway def1 bypass-dhcp"
push "dhcp-option DNS 10.29.112.168"
push "dhcp-option NTP 10.29.112.168"
push "route 10.29.112.0 255.255.255.224" 
push "route 10.29.113.0 255.255.255.224" 
push "route 10.29.114.0 255.255.255.128" 
push "route 172.23.0.0 255.255.224.0" 
push "route 172.23.64.0 255.255.224.0" 
push "route 172.23.96.0 255.255.224.0" 
push "route 10.29.113.32 255.255.255.224" 
push "route 10.29.112.160 255.255.255.240" 
client-to-client
keepalive 10 300
max-clients 4096
user nobody
group nogroup
persist-key
persist-tun
status /var/log/openvpn/openvpn-status.log
log-append  /var/log/openvpn/openvpn.log
verb 6
management 0.0.0.0 7777
remote-cert-tls client
explicit-exit-notify 1
#tls-version-min 1.2
ns-cert-type client

#auth sha256
#tls-cipher "TLS-ECDHE-ECDSA-WITH-AES-128-GCM-SHA256:TLS-ECDHE-ECDSA-WITH-AES-128-CBC-SHA256:TLS-ECDHE-RSA-WITH-AES-128-GCM-SHA256:TLS-ECDHE-RSA-WITH-AES-128-CBC-SHA256:TLS-DHE-RSA-WITH-AES-128-GCM-SHA256:TLS-DHE-RSA-WITH-AES-128-CBC-SHA256"
#ncp-ciphers AES-256-GCM:AES-192-GCM:AES-128-GCM
script-security 2
